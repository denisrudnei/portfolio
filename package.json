  "author": "Denis Rudnei de Souza",
  "private": true,
  "engines": {
    "node": ">=13.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --transpile-only --respawn --ignore-watch node_modules server/index.ts",
    "build": "nuxt build && tsc -p tsconfig-build.json",
    "tsc": "tsc -p tsconfig-build.json",
    "start": "cross-env NODE_ENV=production node -r tsconfig-paths/register dist/server/index.js",
    "heroku-postbuild": "npm run build",
    "generate": "nuxt generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
